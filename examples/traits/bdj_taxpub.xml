<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="file:/home/thc4/Github/TaxPub/examples/traits/Szero_taxpub.rnc" type="application/relax-ng-compact-syntax"?>
<tp:taxon-treatment xmlns:tp="http://www.plazi.org/taxpub" xmlns:xlink="http://xlink.org">
    <tp:nomenclature>
        <tp:taxon-name>Scytodes thoracica</tp:taxon-name>
        <tp:taxon-status>n.sp.</tp:taxon-status>
    </tp:nomenclature>
    <tp:treatment-sec sec-type="Citations">
        <p>
            <tp:trait content-type="Character"><tp:trait-part
                    content-type="CharacterTitle"><object-id xlink:href="dwc:basisOfRecord"
                    /></tp:trait-part><tp:trait-part
                    content-type="characterValue"/>Holotype</tp:trait>
        </p>
    </tp:treatment-sec>
    <tp:treatment-sec sec-type="Diagnosis">
        <title>Diagnosis</title>
        <p>
            <tp:trait content-type="Character"><tp:trait-part
                    content-type="CharacterTitle"
                    >head</tp:trait-part><tp:trait-part
                    content-type="CharacterValue" measure="color"
                    >black</tp:trait-part><tp:trait-part
                    content-type="Remarks">variation of black to
                    grey</tp:trait-part></tp:trait>
        </p>
        <p>
            <tp:trait content-type="Character"><tp:trait-part
                    content-type="CharacterTitle"
                >body</tp:trait-part></tp:trait>
        </p>
    </tp:treatment-sec>
    <tp:treatment-sec sec-type="Description">
        <title>Morphology</title>
        <p>
            <tp:trait content-type="Character"><tp:trait-part
                content-type="CharacterTitle"
                >size</tp:trait-part><tp:trait-part
                    content-type="CharacterValue" measure="mm">3</tp:trait-part>
            </tp:trait>
        </p>
    </tp:treatment-sec>
    <tp:treatment-sec sec-type="Description">
        <title>Head</title>
        <p>
            <tp:trait content-type="Character"><tp:trait-part
                content-type="CharacterTitle"
                >size</tp:trait-part><tp:trait-part
                    content-type="CharacterValue" measure="mm">3</tp:trait-part>
            </tp:trait>
        </p>
    </tp:treatment-sec>

</tp:taxon-treatment>
